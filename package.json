{
  "name": "web-inspector-recorder",
  "version": "1.0.0",
  "description": "Record and generate Playwright test scripts automatically",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "show-report": "playwright show-report"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "playwright": "^1.42.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "recorder"
  ],
  "author": "",
  "license": "MIT"
}
